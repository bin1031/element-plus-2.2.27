{
  "name": "@element-plus/tokens",
  "version": "3.0.0",
  "description": "Element Plus tokens",
  "homepage": "https://github.com/element-plus/element-plus#readme",
  "bugs": {
    "url": "https://github.com/element-plus/element-plus/issues"
  },
  "license": "MIT",
  "author": "Element Plus Team <hello@element-plus.org>",
  "files": [
    "dist"
  ],
  "sideEffects": false,
  "main": "src/index.ts",
  "module": "src/index.ts",
  "unpkg": "index.js",
  "jsdelivr": "index.js",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/element-plus/element-plus.git",
    "directory": "packages/tokens"
  },
  "scripts": {
    "clean": "rimraf dist",
    "build": "tsup src --dts",
    "dev": "tsup src --watch",
    "prepack": "clean-package",
    "postpack": "clean-package restore"
  },
  "clean-package": "../../clean-package.json",
  "peerDependencies": {
    "vue": "^3.2.0"
  },
  "dependencies": {
    "@element-plus/components": "workspace:*",
    "@element-plus/constants": "workspace:*",
    "@element-plus/hooks": "workspace:*",
    "@element-plus/utils": "workspace:*",
    "vue": "^3.2.0"
  },
  "devDependencies": {
    "clean-package": "^2.2.0"
  },
  "tsup": {
    "clean": true,
    "target": "es2018",
    "format": [
      "cjs",
      "esm"
    ]
  }
}
